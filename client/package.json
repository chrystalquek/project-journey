{
  "name": "journey-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "cross-env-shell NODE_ENV=production \"shx rm -rf ./build && next build\"",
    "deploy": "npm run build && npm run gcp-predeploy && gcloud app deploy",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "gcp-predeploy": "find ./build -name '\\[...*' -exec bash -c 'mv \"$1\" \"${1/.../@@@}\"' -- {} \\;",
    "gcp-build": "find ./build -name '\\[@@@*' -exec bash -c 'mv \"$1\" \"${1/@@@/...}\"' -- {} \\;",
    "lint-fix": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "next start -p 8080",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@date-io/date-fns": "^1.3.13",
    "@date-io/dayjs": "^1.3.13",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.2.10",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/yup": "^0.29.11",
    "antd": "^4.9.1",
    "axios": "^0.21.0",
    "classnames": "^2.2.6",
    "dayjs": "^1.9.7",
    "fontsource-roboto": "^3.0.3",
    "formik": "^2.2.6",
    "formik-material-ui": "^3.0.1",
    "formik-material-ui-pickers": "^0.0.12",
    "jsonwebtoken": "^8.5.1",
    "next": "^10.0.5",
    "next-redux-wrapper": "^6.0.2",
    "react": "16.13.1",
    "react-avatar-editor": "^11.1.0",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/react-redux": "^7.1.14",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "shx": "^0.3.3",
    "typescript": "^4.0.2"
  }
}
